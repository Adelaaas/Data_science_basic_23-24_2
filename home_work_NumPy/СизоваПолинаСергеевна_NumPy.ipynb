Задача 5
def find_most_frequent(arr):
count = {}
max_count = 0
most_frequent = []
for num in arr:
    if num in count:
        count[num] += 1
     else:
        count[num] = 1
  for num in count:
        if count[num] > max_count:
            max_count = count[num]
            most_frequent = [num]
      elif count[num] == max_count:
            most_frequent.append(num)
   return most_frequent
array = [1, 2, 3, 2, 3, 4, 5, 1, 2, 2, 4]
result = find_most_frequent(array)
print(result)

